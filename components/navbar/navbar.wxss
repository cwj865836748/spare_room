/* components/navbar/index.wxss */
.navbar {
  width: 100%;
  position: relative;
  z-index: 5;
}
.navbarFixed {
  width: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 5;
}
.title-container {
  display: flex;
  align-items: center;
  position: relative;
}
.title_head {
  position: absolute; 
  transform: translate(-50%, -50%); 
  left: 50%; 
  top: 50%; 
  font-size: 17px; 
  font-weight: bold;
  max-width: 400rpx;
}
.slotContent {
  overflow: auto;
  position: relative;
}