.require {
  padding: 22rpx 30rpx 24rpx;
  position: relative;
  background: #f9f9f9;
}

.require>.name {
  color: #333333;
  text-align: center;
  font-weight: bold;
  font-size: 32rpx;
  margin-bottom: 22rpx;
  position: relative;
}

.require>.name>image {
  content: "";
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  right: 240rpx;
  top: 8rpx;

}

.require>.title {
  font-size: 26rpx;

  font-weight: bold;
  text-align: center;

  color: #666666;
  letter-spacing: 2rpx;
  margin-bottom: 24rpx;
}

.require>image {
  width: 26rpx;
  height: 26rpx;
  position: absolute;
  right: 32rpx;
  top: 32rpx;
}
.requireContent {
  
  padding: 0 30rpx 12rpx;
  background: #f9f9f9;
}
.requireBtn {
  width: 100%;
  height: 84rpx;
  background: var(--buttonColor);
  border-radius: 8rpx;
  font-size: 32rpx;
  color: #ffffff;
  letter-spacing: 2rpx;
  font-weight: bold;
  margin-top: 32rpx;
}

.van-cell {
  height: 100rpx;
  padding: 30rpx 20rpx !important;
  background: rgba(249, 249, 249, 0);
  border-bottom: 2rpx solid #f9f9f9;
  display: flex;
  align-items: center;
  background: #fff;
}
.van-field__label {
  color: #646566 !important;
}

.van-field__input {
  font-size: 26rpx;
  color: #333333;
  letter-spacing: 2rpx;
  font-weight: bold;
  width: 442rpx !important;
}

.van-cell::after {
  display: none;
}

.van-cell__title {
  font-size: 26rpx;
  font-weight: bold;
  color: #646566;
  letter-spacing: 2rpx;
  max-width: 160rpx !important;
  min-width: 160rpx !important;

}

.searchInput>image {
  width: 48rpx;
  height: 48rpx;
  position: absolute;
  right: 6rpx;
  top: 28rpx;

}

.searchInput>.select {
  width: 518rpx;
  background: #ffffff;
  border-radius: 8rpx;
  box-shadow: 0rpx 6rpx 12rpx 0rpx rgba(0, 0, 0, 0.09);
  position: absolute;
  top: 100rpx;
  left: 160rpx;
  z-index: 103;
  padding: 24rpx 34rpx 28rpx 38rpx;
  font-size: 26rpx;
  color: #333333;
  letter-spacing: 2rpx;
  /* box-sizing: content-box; */
  overflow: auto;
  height: 280rpx;
}

.searchInput>.select>view {
  margin-bottom: 34rpx;
}

.searchInput>.select>.seleceNone {
  font-size: 26rpx;
  color: #999999;
  letter-spacing: 2rpx;
  line-height: 40rpx;
}

.searchInput>.select>view:last-child {
  margin-bottom: 0;
}

.van-icon__image {
  width: 31rpx !important;
  height: 35rpx !important;
}

.van-stepper__input {
  border: 2rpx solid rgba(91, 9, 151, .2) !important;
  background: #ffffff !important;
  color: #55059e !important;
  letter-spacing: 2rpx !important;
  font-size: 26rpx !important;
  font-weight: bold !important;
}

.van-stepper__minus,
.van-stepper__plus {
  background: #55059e !important;
  border-radius: 0rpx 4rpx 4rpx 0rpx !important;
  color: #ffffff !important;
  width: 68rpx !important;
}

.van-stepper__minus--disabled,
.van-stepper__plus--disabled {
  background: rgba(85, 5, 158, .2) !important;
  border-radius: 4rpx 0rpx 0rpx 4rpx !important;
  color: #ffffff !important;
  
}

.overlay {
  width: 690rpx;
  height: 318rpx;
  background: #ffffff;
  border-radius: 20rpx;
  padding: 52rpx 52rpx 32rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  text-align: center;

}

.overlay>.msg {
  font-size: 28rpx;
  color: #333333;
  line-height: 54rpx;

}

.overlay>.msg-grey {
  color: rgba(170, 170, 170, 1) !important;
}

.overlay>view:last-child {
  margin-top: 26rpx;
  text-align: center;
  color: #8c42ad;
  font-size: 32rpx;
}

.myCellGroup {
  margin-bottom: 24rpx;
  border-radius: 8rpx;
  /* overflow: hidden; */
}

.myBreadfast {
  width: 306rpx;
  height: 60rpx;
  background: #ffffff;
  border: 2rpx solid #cccccc;
  border-radius: 4rpx;
  padding: 10rpx 20rpx;
  color: #aaaaaa;
  font-size: 26rpx;
}
.myBreadfast>image {
  width: 16rpx;
  height: 16rpx;
}
.requireBody {
  padding-top: 284rpx;
  font-size: 24rpx;
  color: #999999;
 line-height: 44rpx;
 letter-spacing: 1rpx;
}
.requireBody>image {
  width: 242rpx;
  height: 160rpx;
}
.requireTitle {
  width: 548rpx;
  text-align: center;
  height: 100rpx;
  margin-bottom: 10rpx;
}
.requireBtns {
  width: 208rpx;
height: 84rpx;
border: 2rpx solid #aaaaaa;
border-radius: 8rpx;
color: #333333;
letter-spacing: 2rpx;
font-size: 26rpx;
}
.van-field__label--disabled {
  color: #646566 !important;
}
.van-field__input--disabled {
  color: #323233 !important;
}
.release {
  padding: 32rpx 0 52rpx;
  box-sizing: content-box;
  background: #f9f9f9;
}

.release>view {
  width: 222rpx;
  height: 72rpx;
  background: #ffffff;
  border: 2rpx solid #cccccc;
  border-radius: 8rpx;
  font-size: 30rpx;
  color: #666666;
  letter-spacing: 2rpx;
  font-weight: bold;
}