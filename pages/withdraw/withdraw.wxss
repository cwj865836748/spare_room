/* pages/withdraw/withdraw.wxss */
.warn {
  height: 54rpx;
  background: #ffe8e5;
  font-size: 26rpx;
  color: #e3311a;
  letter-spacing: 2rpx;
}

.warn>image {
  width: 31rpx;
  height: 31rpx;
  margin-right: 8rpx;
}

.withdrawContent {
  padding: 24rpx 30rpx 0;
}

.goWithdraw {
  height: 114rpx;
  background: rgba(249, 249, 249, 0);
  padding: 0 30rpx;
  font-size: 26rpx;
  color: #333333;
  letter-spacing: 2rpx;
  border-bottom: 2rpx solid #f9f9f9;
}



.goWithdraw>.money>text:first-child {
  color: #6707be;
  letter-spacing: 1rpx;
  font-size: 24rpx;
  font-weight: bold;
}

.goWithdraw>text:last-child {
  color: #6707be;
  letter-spacing: 2rpx;
  font-size: 26rpx;
  font-weight: bold;
}

.van-cell {
  padding-left: 0 !important;
}
.moneyOut {
  margin-top: 14rpx;
  padding-right: 30rpx;
  text-align: right;
  color: #6707be;
letter-spacing: 2rpx;
font-size: 26rpx;
}
.moneyBtn {
  height: 88rpx;
background: var(--buttonColor);
border-radius: 8rpx;
font-size: 32rpx;
color: #ffffff;
letter-spacing: 2rpx;
margin-top: 136rpx;
}
.van-field__input {
  color: #6707be !important;
}
.dialog {
  padding: 48rpx 50rpx 52rpx 36rpx;
  letter-spacing: 1rpx;
  color: #333333;
  font-size: 28rpx;
}