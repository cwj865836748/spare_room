bottom: -46rpx;

} */
.hotelHead .distance>.disContent>.meter>view {
  height: 80rpx;
  background: rgba(249, 249, 249, 0);
  border-bottom: 1px solid #f2f2f2;
  font-size: 28rpx;
  color: #666666;
  letter-spacing: 2rpx;
  padding-left: 200rpx;
}

.hotelHead .distance>.disContent>.meter>view:nth-child(n+2) {
  margin-top: 20rpx;
}

.hotelHead .distance>.disContent>.meter {
  width: 546rpx;
  height: 480rpx;
}

.hotelHead .distance>.piao {
  margin-top: 30rpx;
  padding: 0 40rpx 0 38rpx;
}

.hotelHead .comprehensive {
  padding: 24rpx 0rpx 30rpx 0rpx;
}

.hotelHead .comprehensive>.disContent>.group {
  padding: 12rpx 0rpx 32rpx 28rpx;
  font-size: 24rpx;
  color: #999999;
  letter-spacing: 1rpx;
  width: 562rpx;
  border-bottom: 2rpx solid #f2f2f2;
  border-left: 2rpx solid #f2f2f2;
  margin-bottom: 30rpx;
}

.hotelHead .comprehensive>.disContent>.group {
  padding: 12rpx 0rpx 28rpx 28rpx;
  font-size: 24rpx;
  color: #999999;
  letter-spacing: 1rpx;
  width: 562rpx;
  border-bottom: 2rpx solid #f2f2f2;
  border-left: 2rpx solid #f2f2f2;
  margin-bottom: 30rpx;
}

.hotelHead .comprehensive>.disContent>.group>.title {
  margin-bottom: 20rpx;
}

.hotel {
  width: 100%;
}

.hotelHead .comprehensive>.disContent>.promotion {
  padding: 30rpx 108rpx 74rpx 30rpx;
  font-size: 24rpx;
  letter-spacing: 0rpx;
  width: 562rpx;
  border-bottom: 2rpx solid #f2f2f2;
  border-left: 2rpx solid #f2f2f2;
  margin-bottom: 14rpx;
}

.hotelHead .comprehensive>.disContent>.promotion>view {
  padding: 0rpx 12rpx;
  height: 52rpx;
  background: #ffffff;
  border-radius: 26rpx;
  font-size: 24rpx;
  letter-spacing: 1rpx;
  box-sizing: content-box;
  margin: 0 20rpx 28rpx 0;
}

.purple {
  border: 2rpx solid #780acc !important;
  color: #780ACC !important;
  background: #ffffff !important;
}

.grey {
  border: 2rpx solid #666666;
  color: #666666;
}

.orange {
  border: 2rpx solid #ff8c38;
  color: #ff8c38;
}

.hotelList {
  padding: 212rpx 30rpx 0;
  min-height: 100%;
}

.hotelList>.hotelNone {
  height: 490rpx;
  font-size: 24rpx;
  color: #999999;
  line-height: 44rpx;
  letter-spacing: 1rpx;
  padding: 142rpx 72rpx 0;
}

.hotelList>.hotelNone>image {
  width: 242rpx;
  height: 160rpx;
}

.hotelList>.hotelNone>.fbQuire {
  width: 208rpx;
  height: 84rpx;
  border: 2rpx solid #aaaaaa;
  border-radius: 8rpx;
  font-size: 26rpx;
  font-weight: bold;
  color: #333333;
  letter-spacing: 2rpx;
}

.returnTop {
  position: fixed;
  right: 5%;
  bottom: 5%;
  height: 42px;
  width: 42px;
  transition: all 0.5s;
  z-index:1
}
.returnTop>image {
  width: 100%;
  height: 100%;
}